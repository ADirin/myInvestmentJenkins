pipeline {
  agent any
    environment {
    PATH = "${env.PATH};C:\\Windows\\System32" // Update the PATH to include the directory of cmd.exe
    }
     stages {
      stage('checkout') {
          steps{
              git branch: 'master', credentialsId: 'ADirin',  url: 'https://github.com/ADirin/myInvestmentJenkins.git'
          }
          
      } 
      stage ('Build') {
          steps{
            bat 'mvn clean install'
          }
      }
    
  }

}
