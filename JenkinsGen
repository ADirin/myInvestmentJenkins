pipeline {
  agent any 
     stages {
      stage('checkout') {
          steps{
              git branch: 'master', credentialsId: 'ADirin',  url: 'https://github.com/ADirin/myInvestmentJenkins.git'
          }
          
      } 
      stage ('Build') {
          steps{
            bat 'mvn clean install'
          }
      }
    
  }

}
